{
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "rootDirectory": "frontend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://localhost:8545/:path*"
    }
  ]
}
